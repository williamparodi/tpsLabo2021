#include <stdio.h>
#include <stdlib.h>

float multiplicarDosNumeros(float numeroUno, float numeroDos);

int main(void)
{
	float numeroUno;
		float numeroDos;
		float resultadoSuma;
		float resultadoResta;
		float resultadoMultiplicacion;
		float resultadoDivision;

		 resultadoMultiplicacion = multiplicarDosNUmeros(numeroUno,numeroDos);

		printf("La multiplicacion de %.2f * %.2f es : %f \n",numeroUno,numeroDos,resultadoMultiplicacion);

	return EXIT_SUCCESS;
}

float multiplicarDosNumeros(float numeroUno, float numeroDos)
	{
		float resultado;
		resultado = numeroUno * numeroDos;
		return resultado;
	}


